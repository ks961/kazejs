{
  "name": "@d3vtool/kazejs",
  "version": "1.9.24",
  "source": "src/kaze/index.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "minify:esm": "esbuild dist/esm/**/*.js --minify --outdir=dist/esm --allow-overwrite",
    "minify:cjs": "esbuild dist/cjs/**/*.js --minify --outdir=dist/esm --allow-overwrite",
    "build": "npm run build:esm && npm run build:cjs && npm run minify:esm && npm run minify:cjs",
    "test": "vitest"
  },
  "keywords": [
    "kaze",
    "kazejs",
    "kaze-js",
    "routing",
    "middleware",
    "dependency-injection",
    "schema-validation",
    "async-handling",
    "static-files",
    "global-error-handling",
    "route-grouping",
    "http-server",
    "typescript",
    "api-server",
    "validator",
    "json-body",
    "cookie-parser",
    "query-validation",
    "params-validation",
    "rest-api",
    "web-server"
  ],
  "author": "Sudhanshu Kumar",
  "license": "MIT",
  "description": "A flexible Node.js web framework built with TypeScript, focusing on dependency injection, routing, middleware, and schema validation. Supports dynamic routes, global middleware, static files, and customizable error handling for scalable apps.",
  "devDependencies": {
    "@types/node": "^22.10.5",
    "esbuild": "^0.25.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.2"
  },
  "dependencies": {
    "@d3vtool/secid": "^1.1.7",
    "@d3vtool/utils": "^8.1.1"
  },
  "files": [
    "dist"
  ]
}
